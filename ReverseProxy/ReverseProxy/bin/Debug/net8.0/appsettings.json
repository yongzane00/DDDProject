{
  "ReverseProxy": {
    "Routes": {
      "orders-api": {
        "ClusterId": "orders-cluster",
        "Match": {
          "Path": "api/orders/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "orders-cluster": {
        "Destinations": {
          "orders-api": {
            "Address": "http://localhost:5247/"
          }
        }
      }
    }
  }
}
